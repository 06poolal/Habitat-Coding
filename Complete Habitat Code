                     NA,

              ifelse((H$WFBS_L1=="M" & H$WFBS_L2=="01" & H$level4a==1) # MXX01 ----
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="02" & H$level4a==1)
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="04" & H$level4a==1)
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="01" & H$level4a==0 & H$level4b==1) 
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="02" & H$level4a==0 & H$level4b==1)
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="04" & H$level4a==0 & H$level4b==1)
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="01" & is.na(H$level4a) & H$level4b==1) 
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="02" & is.na(H$level4a) & H$level4b==1)
                     |(H$WFBS_L1=="M" & H$WFBS_L2=="04" & is.na(H$level4a) & H$level4b==1)
                     ,"01",

              ifelse((H$WFBS_L1=="M" & H$WFBS_L2=="01" & H$level4a==2) # MXX03 ----
