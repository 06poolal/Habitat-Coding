                  "Beans",
            ifelse(H$WFBS_L1=="P" & H$level2==2 & is.na(H$level4a) & H$level4a==10 & H$rn10>with(soc,Cut.off.N05[match(H$govreg,Region)]) &
                       H$rn10<=with(soc,Cut.off.N04[match(H$govreg,Region)]),
                 "Linseed",
            ifelse(H$WFBS_L1=="P" & H$level2==2 & is.na(H$level4a) & H$level4a==10 & H$rn10>with(soc,Cut.off.N04[match(H$govreg,Region)]),
                  "Rest",
            ifelse(H$WFBS_L1=="P" & H$level2==3 & is.na(H$level4a) & H$level4a==10 & H$rn10<=with(soc,Cut.off.N05[match(H$govreg,Region)]),
                  "Beans",
            ifelse(H$WFBS_L1=="P" & H$level2==3 & is.na(H$level4a) & H$level4a==10 & H$rn10>with(soc,Cut.off.N05[match(H$govreg,Region)]) &
                        H$rn10<=with(soc,Cut.off.N04[match(H$govreg,Region)]),
                  "Linseed",
            ifelse(H$WFBS_L1=="P" & H$level2==3 & is.na(H$level4a) & H$level4a==10 & H$rn10>with(soc,Cut.off.N04[match(H$govreg,Region)]),
                   "Rest",
            ifelse(H$WFBS_L1=="P" & H$level2==4 & is.na(H$level4a) & H$level4a==10 & H$rn10<=with(soc,Cut.off.N05[match(H$govreg,Region)]),
                  "Beans",
            ifelse(H$WFBS_L1=="P" & H$level2==4 & is.na(H$level4a) & H$level4a==10 & H$rn10>with(soc,Cut.off.N05[match(H$govreg,Region)]) &
                       H$rn10<=with(soc,Cut.off.N04[match(H$govreg,Region)]),
                 "Linseed",
            ifelse(H$WFBS_L1=="P" & H$level2==4 & is.na(H$level4a) & H$level4a==10 & H$rn10>with(soc,Cut.off.N04[match(H$govreg,Region)]),
                  "Rest",
            ifelse(H$WFBS_L1=="P" & H$level2==6 & is.na(H$level4a) & H$level4a==10 & H$rn10<=with(soc,Cut.off.N05[match(H$govreg,Region)]),
