###                          ###

### WINTER HABITAT ALGORITHM ###
###                          ###


## Loading Data Frames ## ----
##


rm( list = ls() )
suppressMessages(library(dplyr))
#suppressMessages(library(data.table))

#workingdir <- "C:/Users/Poole.a/Documents/FOR_ALEX/Habitat data_BGRI regions.csv"
#setwd(workingdir)
hab <- read.csv("~/MSc/Work Placement/Placement/Habitat data_BGRI regions.csv", header=T, na.strings =c("",NA))

#Renaming all of the column titles to lowecase shortened versions
names(hab)[names(hab) =='GRIDREF'] <- "gridref"
names(hab)[names(hab) =='TRANSECT'] <- "trans"
names(hab)[names(hab) =='DURATION_HRS'] <- "dur"
names(hab)[names(hab) =='OBS_DT'] <- "obsdate"
names(hab)[names(hab) =='DATEUK'] <- "dateuk"
names(hab)[names(hab) =='VISIT'] <- "visit"
names(hab)[names(hab) =='REGION_CODE'] <- "regcode"
names(hab)[names(hab) =='REGION_NAME'] <- "county"
names(hab)[names(hab) =='OLDCOUNTY'] <- "oc"
names(hab)[names(hab) =='OLDCOUNTY_NAME'] <- "ocn"
names(hab)[names(hab) =='SUB_ID'] <- "subid"
names(hab)[names(hab) =='HABITAT_NUM'] <- "habno"
names(hab)[names(hab) =='STATUS'] <- "status"
names(hab)[names(hab) =='LEVEL_1'] <- "level1"
names(hab)[names(hab) =='LEVEL_2'] <- "level2"
names(hab)[names(hab) =='LEVEL_3A'] <- "level3a"
names(hab)[names(hab) =='LEVEL_3B'] <- "level3b"
names(hab)[names(hab) =='LEVEL_4A'] <- "level4a"
names(hab)[names(hab) =='LEVEL_4B'] <- "level4b"
names(hab)[names(hab) =='LEVEL_3C'] <- "level3c"
names(hab)[names(hab) =='LEVEL_3D'] <- "level3d"
names(hab)[names(hab) =='LEVEL_4C'] <- "level4c"
names(hab)[names(hab) =='LEVEL_4D'] <- "level4d"
names(hab)[names(hab) =='DISTANCE'] <- "dist"

#str(hab)
#loading lookup table for the regions
c2r <- read.csv("~/MSc/Work Placement/Placement/bbs county to region.csv")
hab$govreg <- with(c2r,GovRegion[match(hab$county,BBS.Counties)])

# filter - only want early visits (change this to L if you wish to use late visits instead)
earlyhab <- hab[which(hab$visit=="E"),]
earlyhab <- within(earlyhab, rm(visit))
rm(hab)

#Filter - only actual recordings (A) to used not ideal (I)
stathab <- earlyhab[which(earlyhab$status=="A"),]
rm(earlyhab)

#Remove - removed the columns with only 1 level in them now (visit and status)
stathab <- within(stathab, rm(status))

#Remove all but E (Farmland) habitats, and then remove Level1 column
farmhab <- stathab[which(stathab$level1=="E"),]
farmhab <- within(farmhab, rm(level1))
rm(stathab)
#row.names(farmhab) <- 1:nrow(farmhab)
#head(farmhab, n=20)

#Ordering the columns by gridref, then date, then transect number, then habitat number
farmhab$date <- strptime(as.character(farmhab$dateuk), "%d/%m/%Y")
farmhab <- farmhab[order(farmhab$gridref,  farmhab$date, farmhab$habno, farmhab$trans ),]
#head(farmhab)

#df with columns excluded
H <- within(farmhab, rm(dur,obsdate,dateuk,regcode,county,oc,ocn,subid,dist,level3c, level3d, level4c, level4d  ))
#head(H)
rm(farmhab)

# ----


## random numbers columns ## ----
H$rn1 <- runif(1:nrow(H), min=0, max=100) #Using for Bare Earth or Stubble
H$rn2 <- runif(1:nrow(H), min=0, max=100) #Using for Spring or Winter Other Crop (BBS)
H$rn3 <- runif(1:nrow(H), min=0, max=100) #Using for Spring or Winter OSR
H$rn4 <- runif(1:nrow(H), min=0, max=100) #Using for Winter Cereal proportions
H$rn5 <- runif(1:nrow(H), min=0, max=100) #Using for Any Stubble proportions
H$rn6 <- runif(1:nrow(H), min=0, max=100) #Using for OSR/Bean Stubble proportions
H$rn7 <- runif(1:nrow(H), min=0, max=100) #Using for Sugarbeet/Potato crop proportions (SPInter)
H$rn8 <- runif(1:nrow(H), min=0, max=100) #Using for Sugarbeet/Potato stubble proportions
H$rn9 <- runif(1:nrow(H), min=0, max=100) #Using for Winter Beans/Linseed proportions
H$rn10 <- runif(1:nrow(H), min=0, max=100) #Using for Spring Beans/Linseed/Rest Other Crops proportion
H$rn11 <- runif(1:nrow(H), min=0, max=100) #Using for Linseed stubble proportion
H$rn12 <- runif(1:nrow(H), min=0, max=100) #Using for Clean/Weedy Stubble L3a
H$rn13 <- runif(1:nrow(H), min=0, max=100) #Using for Brassica preceeding
H$rn14 <- runif(1:nrow(H), min=0, max=100) #Using for improved/unimproved grassland
H$rn15 <- runif(1:nrow(H), min=0, max=100) #Using for Brassica stubble
H$rn16 <- runif(1:nrow(H), min=0, max=100) #Using for Clean/Weedy Stubble L3b
H$rn17 <- runif(1:nrow(H), min=0, max=100) #Using for Stubble/Bare Earth L3b



# ----


## Importing the lookup tables for the proportions ## ----
stb <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Stubble.Bare Earth.csv")
swoc <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/S.W BBS Other Crops.csv")
swosr <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/S.W OSR.csv")
wc <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/W.Cereals.csv")
as <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Any Stubble.csv")
obs <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/OSR.Bean Stub.csv")
sp <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Sb.Pot.csv")
sbs <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Sb Stub.csv")
pos <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Pot Stub.csv")
bls <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Bean.Lin.csv")
soc <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/OC Inter.csv")
lis <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Lin Stub.csv")
bras <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Brassica.csv")
cws <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/C.W Stubble.csv")
bst <- read.csv("~/MSc/Work Placement/Placement/Proportions CSV's/Bras Stub.csv")

