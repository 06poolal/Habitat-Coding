                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==4 & H$level4a==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==6 & H$level4a==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==6 & H$level4a==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==1 & H$level4a==0 & H$level4b==10 & H$SOCInter=="Beans" &
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==1 & H$level4a==0 & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==2 & H$level4a==0 & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==2 & H$level4a==0 & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==3 & H$level4a==0 & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==3 & H$level4a==0 & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==4 & H$level4a==0 & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==4 & H$level4a==0 & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==6 & H$level4a==0 & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==6 & H$level4a==0 & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==1 & is.na(H$level4a) & H$level4b==10 & H$SOCInter=="Beans" &
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==1 & is.na(H$level4a) & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==2 & is.na(H$level4a) & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==2 & is.na(H$level4a) & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==3 & is.na(H$level4a) & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==3 & is.na(H$level4a) & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==4 & is.na(H$level4a) & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==4 & is.na(H$level4a) & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==6 & is.na(H$level4a) & H$level4b==10 & H$SOCInter=="Beans" & 
                        H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    |(H$WFBS_L1=="P" & H$level2==6 & is.na(H$level4a) & H$level4b==11 & H$rn6>with(obs,Cut.Off.P07[match(H$govreg,Region)]) & 
                        H$rn6<=with(obs,Cut.Off.P09[match(H$govreg,Region)]))
                    ,"09",

             "other")))))))))))))))))))))))))))))))))))))))))))))) #----

