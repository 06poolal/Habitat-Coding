                     ,"09",

              ifelse((H$WFBS_L1=="Q" & H$WFBS_L2=="07" & H$level4a==6) # Q0701 ----
                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="07" & H$level4a==7)
