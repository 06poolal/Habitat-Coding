                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="07" & is.na(H$level4a) & H$level4b==14)
                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="07" & H$level4a==1)
                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="07" & H$level4a==16)
