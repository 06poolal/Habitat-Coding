### Level 1 Code ### ----
##


H$WFBS_L1 <- ifelse((is.na(H$level2)) # NA ----
                    |(H$level2==0)
                    |(H$level2>=7)
                    |(H$level4a==0 & H$level4b==0)
                     ,NA,

              ifelse((H$level2==1 & H$level4a==1) # M ----
                     |(H$level2==1 & H$level4a==2)
                     |(H$level2==1 & H$level4a==3)
                     |(H$level2==1 & H$level4a==4)
                     |(H$level2==1 & H$level4a==5)
                     |(H$level2==1 & H$level4a==16)
                     |(H$level2==1 & H$level4a==0 & H$level4b==1)
                     |(H$level2==1 & H$level4a==0 & H$level4b==2)
                     |(H$level2==1 & H$level4a==0 & H$level4b==3)
                     |(H$level2==1 & H$level4a==0 & H$level4b==4)
                     |(H$level2==1 & H$level4a==0 & H$level4b==5)
                     |(H$level2==1 & H$level4a==0 & H$level4b==16)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==1)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==2)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==3)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==4)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==5)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==16)
                     ,"M",

              ifelse((H$level2==2 & H$level4a==1) # M ----
                     |(H$level2==2 & H$level4a==2)
                     |(H$level2==2 & H$level4a==3)
                     |(H$level2==2 & H$level4a==4)
                     |(H$level2==2 & H$level4a==5)
                     |(H$level2==2 & H$level4a==16)
                     |(H$level2==2 & H$level4a==0 & H$level4b==1)
                     |(H$level2==2 & H$level4a==0 & H$level4b==2)
                     |(H$level2==2 & H$level4a==0 & H$level4b==3)
                     |(H$level2==2 & H$level4a==0 & H$level4b==4)
                     |(H$level2==2 & H$level4a==0 & H$level4b==5)
                     |(H$level2==2 & H$level4a==0 & H$level4b==16)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==1)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==2)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==3)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==4)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==5)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==16)
                     ,"M",

              ifelse((H$level2==3 & H$level4a==1) # M ----
                     |(H$level2==3 & H$level4a==2)
                     |(H$level2==3 & H$level4a==3)
                     |(H$level2==3 & H$level4a==4)
                     |(H$level2==3 & H$level4a==5)
                     |(H$level2==3 & H$level4a==16)
                     |(H$level2==3 & H$level4a==0 & H$level4b==1)
                     |(H$level2==3 & H$level4a==0 & H$level4b==2)
                     |(H$level2==3 & H$level4a==0 & H$level4b==3)
                     |(H$level2==3 & H$level4a==0 & H$level4b==4)
                     |(H$level2==3 & H$level4a==0 & H$level4b==5)
                     |(H$level2==3 & H$level4a==0 & H$level4b==16)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==1)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==2)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==3)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==4)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==5)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==16)
                     ,"M",

              ifelse((H$level2==4 & H$level4a==16) # M ----
                     |(H$level2==6 & H$level4a== 16)
                     |(H$level2==4 & H$level4a==0 & H$level4b==16)
                     |(H$level2==6 & H$level4a==0 & H$level4b==16)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==16)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==16)
                     ,"M",

              ifelse((H$level2==4 & H$level4a==1) # Q ----
                     |(H$level2==4 & H$level4a==0 & H$level4b==1)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==1)
                     |(H$level2==6 & H$level4a==1)
                     |(H$level2==6 & H$level4a==0 & H$level4b==1)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==1)
                     ,"Q",

              ifelse((H$level2==4 & H$level4a==2) # P ----
                     |(H$level2==4 & H$level4a==0 & H$level4b==2)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==2)
                     |(H$level2==6 & H$level4a==2) 
                     |(H$level2==6 & H$level4a==0 & H$level4b==2)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==2)
                     ,"P",

              ifelse((H$level2==4 & H$level4a==3) # P ----
                     |(H$level2==4 & H$level4a==0 & H$level4b==3)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==3)
                     |(H$level2==6 & H$level4a==3) 
                     |(H$level2==6 & H$level4a==0 & H$level4b==3)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==3)
                     ,"P",

              ifelse((H$level2==4 & H$level4a==4) # P ----
                     |(H$level2==4 & H$level4a==0 & H$level4b==4)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==4)
                     |(H$level2==6 & H$level4a==4) 
                     |(H$level2==6 & H$level4a==0 & H$level4b==4)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==4)
                     ,"P",

              ifelse((H$level2==4 & H$level4a==5) # P ----
                     |(H$level2==4 & H$level4a==0 & H$level4b==5)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==5)
                     |(H$level2==6 & H$level4a==5)
                     |(H$level2==6 & H$level4a==0 & H$level4b==5)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==5)
                     ,"P",

              ifelse((H$level2==1 & H$level4a==6) # Q ----
                     |(H$level2==2 & H$level4a==6)
                     |(H$level2==3 & H$level4a==6)
                     |(H$level2==4 & H$level4a==6)
                     |(H$level2==6 & H$level4a==6)
                     |(H$level2==1 & H$level4a==0 & H$level4b==6)
                     |(H$level2==2 & H$level4a==0 & H$level4b==6)
                     |(H$level2==3 & H$level4a==0 & H$level4b==6)
                     |(H$level2==4 & H$level4a==0 & H$level4b==6)
                     |(H$level2==6 & H$level4a==0 & H$level4b==6)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==6)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==6)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==6)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==6)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==6)
                     ,"Q",

              ifelse((H$level2==1 & H$level4a==7) # N ----
                     |(H$level2==2 & H$level4a==7)
                     |(H$level2==3 & H$level4a==7)
                     |(H$level2==4 & H$level4a==7)
                     |(H$level2==6 & H$level4a==7)
                     |(H$level2==1 & H$level4a==0 & H$level4b==7)
                     |(H$level2==2 & H$level4a==0 & H$level4b==7)
                     |(H$level2==3 & H$level4a==0 & H$level4b==7)
                     |(H$level2==4 & H$level4a==0 & H$level4b==7)
                     |(H$level2==6 & H$level4a==0 & H$level4b==7)
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==7)
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==7)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==7)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==7)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==7)
                     ,"N",

              ifelse((H$level2==1 & H$level4a==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)])) # P ----
                     |(H$level2==2 & H$level4a==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==1 & H$level4a==0 & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==2 & H$level4a==0 & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==0 & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==0 & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==0 & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==8 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     ,"P", #Stubble as with(stb,Stubble[match(H$govreg,Region)])%

              ifelse((H$level2==1 & H$level4a==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)])) # Q ----
                     |(H$level2==2 & H$level4a==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==1 & H$level4a==0 & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==2 & H$level4a==0 & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==0 & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==0 & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==0 & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==8 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     ,"Q",

              ifelse((H$level2==1 & H$level4a==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)])) # P ----
                     |(H$level2==2 & H$level4a==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==1 & H$level4a==0 & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==2 & H$level4a==0 & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==0 & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==0 & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==0 & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==9 & H$rn1<=with(stb,Stubble[match(H$govreg,Region)]))
                     ,"P",

              ifelse((H$level2==1 & H$level4a==9 & H$rn1>with(stb,Stubble[match(H$govreg,Region)])) # Q ----
                     |(H$level2==2 & H$level4a==9 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==9 & H$rn1>with(stb,Stubble[match(H$govreg,Region)]))
