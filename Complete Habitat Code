                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="07" & is.na(H$level4a) & H$level4b==16)
                     ,"01",

              ifelse((H$WFBS_L1=="Q" & H$WFBS_L2=="07" & H$level4a==15) # Q0702 ----
                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="07" & H$level4a==0 & H$level4b==15)
