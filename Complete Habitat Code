                     |(H$level2==2 & is.na(H$level4a) & H$level4b==13)
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==13)
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==13)
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==13)
                     ,"P",

              ifelse((H$level2==1 & H$level4a==14) # P ----
                  |(H$level2==2 & H$level4a==14)
                  |(H$level2==3 & H$level4a==14)
