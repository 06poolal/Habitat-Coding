                     |(H$WFBS_L1=="Q" & H$WFBS_L2=="02" & is.na(H$level4a) & H$level4b==12)
                     ,"01",

              "other")))))))))))))))))))) # OTHER ----

# ----

### Level 3b Code ### ----

H$WFBS_L3b <- ifelse((is.na(H$WFBS_L1)) # NA ----
                     |(is.na(H$WFBS_L2))
                     |(is.na(H$level4b))
                     |(is.na(H$level4a) & !is.na(H$WFBS_L3a))
                     |(is.na(H$level4a) & !is.na(H$level4b))
                     |(H$level4a==0 & H$WFBS_L3a!=0)
                     |(H$level4b==0)
                     ,NA,

              ifelse((H$WFBS_L1=="M" & H$level4b==1) # MXX01 ----
                     ,"01",

              ifelse((H$WFBS_L1=="M" & H$level4b==2) # MXX03 ----
                     ,"03",

              ifelse((H$WFBS_L1=="M" & H$level4b==3) # MXX04 ----
                     ,"04",

              ifelse((H$WFBS_L1=="M" & H$level4b==4) # MXX05 ----
                     ,"05",

              ifelse((H$WFBS_L1=="M" & H$level4b==5) # MXX06 ----
                     ,"06",

              ifelse((H$WFBS_L1=="M" & H$level4b==6) # Can't have grazing and bare soil # MXXNA ----
                     |(H$WFBS_L1=="M" & H$level4b==7) #Can't have grazing and crop
                     |(H$WFBS_L1=="M" & H$level4b==8) #Can't have grazing and bare soil/stubble
                     |(H$WFBS_L1=="M" & H$level4b==9) 
                     |(H$WFBS_L1=="M" & H$level4b==10) #Can't have grazing and crop/bare soil/stubble
                     |(H$WFBS_L1=="M" & H$level4b==11) 
                     |(H$WFBS_L1=="M" & H$level4b==12) 
                     |(H$WFBS_L1=="M" & H$level4b==13) #Can't have grazing and stubble
                     |(H$WFBS_L1=="M" & H$level4b==14) 
                     |(H$WFBS_L1=="M" & H$level4b==15) #Can't have grazing and fallow
                     ,NA,

              ifelse((H$WFBS_L1=="M" & H$level4b==16) # MXX08 ----
                     ,"08",

              ifelse((H$WFBS_L1=="N" & H$level4b==1) # A crop won't be grazed when it is growing # NXXNA ----
                     |(H$WFBS_L1=="N" & H$level4b==2)
                     |(H$WFBS_L1=="N" & H$level4b==3)
                     |(H$WFBS_L1=="N" & H$level4b==4)
                     |(H$WFBS_L1=="N" & H$level4b==5)
                     |(H$WFBS_L1=="N" & H$level4b==6) #Can't have crop and bare soil
                     |(H$WFBS_L1=="N" & H$level4b==8) #Can't have crop and bare soil/stubble (7/8/12?)
                     |(H$WFBS_L1=="N" & H$level4b==9) 
                     |(H$WFBS_L1=="N" & H$level4b==10 & H$rn2<=with(swoc,Cut.off.for.Spring[match(H$govreg,Region)])) 
                     |(H$WFBS_L1=="N" & H$level4b==11 & H$rn3>with(swosr,Cut.Off.for.WOSR[match(H$govreg,Region)])) 
                     |(H$WFBS_L1=="N" & H$level4b==12 & H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="N" & H$level4b==13) #Can't have crop and stubble
                     |(H$WFBS_L1=="N" & H$level4b==14)
                     |(H$WFBS_L1=="N" & H$level4b==15)
                     |(H$WFBS_L1=="N" & H$level4b==16) #Can't have crop and grass
                     ,NA,

              ifelse((H$WFBS_L1=="N" & H$level4b==7) # NXX02 ----
                     |(H$WFBS_L1=="N" & H$level4b==10 & H$rn2>with(swoc,Cut.off.for.Spring[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="N" & H$level4b==11 & H$rn3<=with(swosr,Cut.Off.for.WOSR[match(H$govreg,Region)]))
                     ,"02",

              ifelse((H$WFBS_L1=="N" & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)])) #NXX03 ----
                     ,"03",

              ifelse((H$WFBS_L1=="P" & H$level4b==1) #Can't have ungrazed stubble # PXXNA ----
                     |(H$WFBS_L1=="P" & H$level4b==6) #Can't have stubble and bare soil
                     |(H$WFBS_L1=="P" & H$level4b==8 & H$rn17>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==9 & H$rn17>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==10 & H$rn2<=with(swoc,Cut.off.for.Spring[match(H$govreg,Region)]) & 
                         H$rn17>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==11 & H$rn3>with(swosr,Cut.Off.for.WOSR[match(H$govreg,Region)]) & 
                         H$rn17>with(stb,Stubble[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) & 
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==15) #Can't have stubble and fallow
                     |(H$WFBS_L1=="P" & H$level4b==16) #Can't have stubble and grass
                     ,NA,

              ifelse((H$WFBS_L1=="P" & H$level4b==8 & H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & # PXX01 ----
                        H$rn16<=with(cws,Clean[match(H$govreg,Region)])) 
                     |(H$WFBS_L1=="P" & H$level4b==9 & H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & 
                         H$rn16<=with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==10 & H$rn2<=with(swoc,Cut.off.for.Spring[match(H$govreg,Region)]) & 
                         H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & H$rn16<=with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==11 & H$rn3>with(swosr,Cut.Off.for.WOSR[match(H$govreg,Region)]) & 
                         H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & H$rn16<=with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==12 & H$rn13<=with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn16<=with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==13)
                     ,"01",

              ifelse((H$WFBS_L1=="P" & H$level4b==8 & H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & # PXX02 ----
                        H$rn16>with(cws,Clean[match(H$govreg,Region)])) 
                     |(H$WFBS_L1=="P" & H$level4b==9 & H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & 
                         H$rn16>with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==10 & H$rn2<=with(swoc,Cut.off.for.Spring[match(H$govreg,Region)]) & 
                         H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & H$rn16>with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==11 & H$rn3>with(swosr,Cut.Off.for.WOSR[match(H$govreg,Region)]) & 
                         H$rn17<=with(stb,Stubble[match(H$govreg,Region)]) & H$rn16>with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==12 & H$rn13<=with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn16>with(cws,Clean[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==14)
                     ,"02",

              ifelse((H$WFBS_L1=="P" & H$level4b==2) # Stubble can be grazed # PXX05 ----
                     |(H$WFBS_L1=="P" & H$level4b==3)
                     |(H$WFBS_L1=="P" & H$level4b==4)
                     |(H$WFBS_L1=="P" & H$level4b==5)
                     ,"05",
                            
              ifelse((H$WFBS_L1=="P" & H$level4b==7) # Stubble and Crop = Undersown # PXX04 ----
                     |(H$WFBS_L1=="P" & H$level4b==10 & H$rn2>with(swoc,Cut.off.for.Spring[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==11 & H$rn3<=with(swosr,Cut.Off.for.WOSR[match(H$govreg,Region)]))
                     |(H$WFBS_L1=="P" & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     ,"04",

              ifelse((H$WFBS_L1=="Q" & H$level4b==2) # QXX09 ----
                     |(H$WFBS_L1=="Q" & H$level4b==3)
                     |(H$WFBS_L1=="Q" & H$level4b==4)
                     |(H$WFBS_L1=="Q" & H$level4b==5)
                     ,"09",

              ifelse((H$WFBS_L1=="Q" & H$level4b==1) # QXX01 ----
                     |(H$WFBS_L1=="Q" & H$level4b==6) 
                     |(H$WFBS_L1=="Q" & H$level4b==7)
                     |(H$WFBS_L1=="Q" & H$level4b==8)
                     |(H$WFBS_L1=="Q" & H$level4b==9)
                     |(H$WFBS_L1=="Q" & H$level4b==10)
                     |(H$WFBS_L1=="Q" & H$level4b==11)
                     |(H$WFBS_L1=="Q" & H$level4b==12)
                     |(H$WFBS_L1=="Q" & H$level4b==13)
                     |(H$WFBS_L1=="Q" & H$level4b==14)
                     |(H$WFBS_L1=="Q" & H$level4b==16)
                     ,"01",

              ifelse((H$WFBS_L1=="Q" & H$level4b==15) # QXX02 ----
                     ,"02",

              "other"))))))))))))))))))) # OTHER ----

# ----

### Concatenating Code ### ----

H$WFBSa <- paste(H$WFBS_L1,H$WFBS_L2,H$WFBS_L3a, sep = "")
H$WFBSb <- paste(H$WFBS_L1,H$WFBS_L2,H$WFBS_L3b, sep = "")
H$WFBS <- paste(H$WFBS_L1,H$WFBS_L2,H$WFBS_L3a,H$WFBS_L3b, sep = "")


WFBS <- data.frame(H$gridref,H$trans,H$habno,H$govreg,H$date,H$WFBS_L1,H$WFBS_L2,H$WFBS_L3a,H$WFBS_L3b,H$WFBSa,H$WFBSb,H$WFBS)

WFBS$cona <- ifelse(is.na(H$WFBS_L1), "",
                    ifelse(is.na(H$WFBS_L2), "",
                           ifelse(is.na(H$WFBS_L3a), "",
                                  H$WFBSa)))

WFBS$conb <- ifelse(is.na(H$WFBS_L1), "",
                    ifelse(is.na(H$WFBS_L2), "",
                           ifelse(is.na(H$WFBS_L3b), "",
                                  H$WFBSb)))

WFBS$con <- ifelse(is.na(H$WFBS_L1), "",
                   ifelse(is.na(H$WFBS_L2), "",
                          ifelse(is.na(H$WFBS_L3a), "",
                                 ifelse(is.na(H$WFBS_L3b), "",
                                 H$WFBS))))

H$WFBSa <- WFBS$cona
H$WFBSb <- WFBS$conb
H$WFBS <- WFBS$con

# ----

