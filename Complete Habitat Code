                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==1 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) & 
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==2 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) & 
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) & 
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Stubble[match(H$govreg,Region)]) &
                         H$rn13<=with(bras,Bare.Earth[match(H$govreg,Region)]))
                     ,"Q",

              ifelse((H$level2==1 & H$level4a==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)])) # N ----
                     |(H$level2==2 & H$level4a==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==1 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==2 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==3 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==4 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==6 & H$level4a==0 & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==1 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==2 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==3 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==4 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     |(H$level2==6 & is.na(H$level4a) & H$level4b==12 & H$rn13>with(bras,Bare.Earth[match(H$govreg,Region)]))
                     ,"N",

              ifelse((H$level2==1 & H$level4a==13) # P ----
                     |(H$level2==2 & H$level4a==13)
                     |(H$level2==3 & H$level4a==13)
                     |(H$level2==4 & H$level4a==13)
