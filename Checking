# Validating WFBSa ----

vala <- read.csv("~/MSc/Work Placement/Placement/Check A.csv", header=T, na.strings =c("",NA))

H$cbbsa <- paste(H$level2,H$level4a, sep = "") # What the BBS code is concatenated

H$validA1 <- with(vala,WFBS.1[match(H$cbbsa,ConBBSa)]) # What the WFBS should be
H$validA2 <- with(vala,WFBS.2[match(H$cbbsa,ConBBSa)])
H$validA3 <- with(vala,WFBS.3[match(H$cbbsa,ConBBSa)])
H$validA4 <- with(vala,WFBS.4[match(H$cbbsa,ConBBSa)])
H$validA5 <- with(vala,WFBS.5[match(H$cbbsa,ConBBSa)])
H$validA6 <- with(vala,WFBS.6[match(H$cbbsa,ConBBSa)])
H$validA7 <- with(vala,WFBS.7[match(H$cbbsa,ConBBSa)])
H$validA8 <- with(vala,WFBS.8[match(H$cbbsa,ConBBSa)])
H$validA9 <- with(vala,WFBS.9[match(H$cbbsa,ConBBSa)])
H$validA10 <- with(vala,WFBS.10[match(H$cbbsa,ConBBSa)])
H$validA11 <- with(vala,WFBS.11[match(H$cbbsa,ConBBSa)])
H$validA12 <- with(vala,WFBS.12[match(H$cbbsa,ConBBSa)])
H$validA13 <- with(vala,WFBS.13[match(H$cbbsa,ConBBSa)])
H$validA14 <- with(vala,WFBS.14[match(H$cbbsa,ConBBSa)])
H$validA15 <- with(vala,WFBS.15[match(H$cbbsa,ConBBSa)])
H$validA16 <- with(vala,WFBS.16[match(H$cbbsa,ConBBSa)])
H$validA17 <- with(vala,WFBS.17[match(H$cbbsa,ConBBSa)])
H$validA18 <- with(vala,WFBS.18[match(H$cbbsa,ConBBSa)])
H$validA19 <- with(vala,WFBS.19[match(H$cbbsa,ConBBSa)])
H$validA20 <- with(vala,WFBS.20[match(H$cbbsa,ConBBSa)])
H$validA21 <- with(vala,WFBS.21[match(H$cbbsa,ConBBSa)])


H$tfa <- ifelse((H$validA1==H$WFBSa) # True/False match
                |(H$validA2==H$WFBSa)
                |(H$validA3==H$WFBSa)
                |(H$validA4==H$WFBSa)
                |(H$validA5==H$WFBSa)
                |(H$validA6==H$WFBSa)
                |(H$validA7==H$WFBSa)
                |(H$validA8==H$WFBSa)
                |(H$validA9==H$WFBSa)
                |(H$validA10==H$WFBSa)
                |(H$validA11==H$WFBSa)
                |(H$validA12==H$WFBSa)
                |(H$validA13==H$WFBSa)
                |(H$validA14==H$WFBSa)
                |(H$validA15==H$WFBSa)
                |(H$validA16==H$WFBSa)
                |(H$validA17==H$WFBSa)
                |(H$validA18==H$WFBSa)
                |(H$validA19==H$WFBSa)
                |(H$validA20==H$WFBSa)
                |(H$validA21==H$WFBSa)
                ,T,                 
               F)

checkA <- data.frame(H$level2,H$level4a,H$cbbsa,H$WFBS_L1,H$WFBS_L2,H$WFBS_L3a,H$WFBSa,H$tfa)


